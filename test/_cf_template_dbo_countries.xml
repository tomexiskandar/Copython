      <connection_string>
        <add conn_str="DRIVER={ODBC Driver 11 for SQL Server};SERVER=LATITUDE;PORT=1443;DATABASE=Bingy;UID=user_name;PWD=password;"/>
      </connection_string>
    </target>
    <column_mapping source="COUNTRY_ID" target="COUNTRY_ID"/>
    <column_mapping source="COUNTRY_NAME" target="COUNTRY_NAME"/>
    <column_mapping source="REGION_ID" target="REGION_ID"/>
  </copy>
</config>
