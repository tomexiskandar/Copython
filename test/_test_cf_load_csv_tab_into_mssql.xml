<?xml version="1.0" encoding="utf-8"?>
<config>
  <description>Load csv file</description>
  <source_type>csv</source_type>
  <target_type>sql_Table</target_type>
  <copy id="seq_routes">
    <source path="E:\DATA\SEQ_GTFS\routes.txt">
      <encoding>utf-8-sig</encoding>
      <has_header>yes</has_header>
      <delimiter>,</delimiter>
      <quotechar>&quot;</quotechar>
    </source>
    <target Schema_name="dbo" table_name="seq_routes">
      <connection_string>
        <add conn_str="DRIVER={ODBC Driver 11 for SQL Server};SERVER=LATITUDE;PORT=1443;DATABASE=GTFS;UID=user_name;PWD=password"/>
      </connection_string>
    </target>
    <column_mapping source="agency_id" target="agency_id"/>
    <column_mapping source="agency_name" target="agency_name"/>
    <column_mapping source="agency_url" target="agency_url"/>
    <column_mapping source="agency_timezone" target="agency_timezone"/>
    <column_mapping source="agency_lang" target="agency_lang"/>
    <column_mapping source="agency_phone" target="agency_phone"/>
  </copy>
</config>